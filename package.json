{
  "name": "expressappsandbox",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "test": "docker-compose -f docker/docker-compose.test.yml up --build --abort-on-container-exit",
    "production": "docker-compose -f docker/docker-compose.yml up -d",
    "shutdown" : "docker-compose -f docker/docker-compose.yml down",
    "build": "docker-compose -f docker/docker-compose.yml build",
    "compile": "babel src -d dist --extensions .ts,.js",
    "compile-start": "npm run compile && node dist/index.js",
    "development": "nodemon -w ./src --exec \"npm run compile-start\"",
    "start-db": "docker run -d -p 27017:27017 mongo"
  },
  "author": "Tyler Nielsen",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "mongodb": "^3.1.13",
    "mongoose": "^5.4.15",
    "typescript": "^3.4.5"
  }, 
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.2.0",
    "@types/express": "^4.16.1",
    "@types/mocha": "^5.2.7",
    "@types/mongodb": "^3.1.32",
    "@types/mongoose": "^5.3.27",
    "@types/supertest": "^2.0.8",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "supertest": "^3.4.2",
    "tape": "^4.10.1",
    "tslint": "^5.16.0"
  }
}
